<h1>User</h1>

<% form_for(@user) do |f| %>
  <%= f.error_messages %>

  <p>
    Login<br />
    <%= f.text_field :login %>
  </p>
  <p>
    E-mail<br />
    <%= f.text_field :email %>
  </p>
  <p>
    Password<br />
    <%= f.password_field :password %>
  </p>
    <%= f.submit 'ok' %>
  </p>
<% end %>

<%= link_to 'back', users_path %>

